@using Eshop2.Models;
@model Eshop2.Models.Product


@{

    Product p=ViewBag.product;
}



<div class="thumbnail" style="background-color:darkgray">
    <img class="border" width="100" height="100" src="~/Content/images/@p.Imgaddress" title="@Html.Encode(p.ProductDes)"/>
    
        <div class="caption">
        <h3 id="thumbnail-label" style="font-family:Verdana;"><span class="product_id_label"></span>@Html.Raw(p.ProductName)</h3>
        </div>
</div>
        

